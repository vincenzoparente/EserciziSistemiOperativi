    "prefix": "t-close: closing unused files (multiple)",
    "body": [
      "/* Chiudo i file descriptors non necessari */",
      "for (${1:j} = 0; ${1:j} < ${2:N}; ${1:j}++)",
      "{",
      "\t$0",
      "}",
      "",
      ""
    ]
