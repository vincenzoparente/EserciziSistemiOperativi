  "t-read: read line": {
    "prefix": "t-read: read line",
    "body": [
      "/* Lettura di ciascun carattere del file linea */",
      "${1:j} = 0;",
      "while (read(fd, &(${2:linea}[${1:j}]), 1) != 0)",
      "{",
      "\t/* Trovato il termine di una linea */",
      "\tif (${2:linea}[${1:j}] == '\\n')",
      "\t{",
      "\t\t$0",
      "\t\t",
      "\t\t${1:j} = 0;",
      "\t}",
      "\telse",
      "\t{",
      "\t\t/* Aggiornamento indice */",
      "\t\t${1:j}++;",
      "\t}",
      "}",
      "",
      ""
    ]
  },
  "t-read: read char": {
