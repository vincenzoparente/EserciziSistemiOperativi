      "if ((fdout = open(${1:filename}, O_CREAT | ${2:O_RDWR} | O_TRUNC)) < 0)",
      "{",
      "\tprintf(\"Errore nella creazione del file\\n\");",
      "\texit($3);",
      "}",
      "",
      "$0"
